<!doctype html><html><head><meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1"><title>Editar participante</title><link rel="stylesheet" href="/css/styles.css"></head><body>
<%- include('partials/nav') %>
<main class="container">
  <div class="card">
    <h1>Editar participante</h1>
    <form method="post" action="/admin/edit/<%= p.id %>">
      <div class="form-group"><label>Nombre</label><input name="nombre" value="<%= p.nombre %>" required></div>
      <div class="form-group"><label>Apellidos</label><input name="apellidos" value="<%= p.apellidos %>" required></div>
      <div class="form-group"><label>Teléfono</label><input name="telefono" value="<%= p.telefono %>"></div>
      <div class="form-group"><label>Calle</label><input name="calle" value="<%= p.calle %>"></div>
      <div class="form-group"><label>Número</label><input name="numero" value="<%= p.numero %>"></div>
      <div class="form-group"><label>Población</label><input name="poblacion" value="<%= p.poblacion %>"></div>
      <div class="form-group"><label>Código postal</label><input name="cp" value="<%= p.cp %>"></div>
      <button class="btn btn-pastel" type="submit">Guardar</button>
    </form>
  </div>
</main>
<footer class="footer"><a class="return" href="/">Volver a Inicio</a></footer></body></html>